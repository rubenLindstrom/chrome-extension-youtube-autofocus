{
  "manifest_version": 2,
  "version": "1.0",
  "name": "YouTube Autofocus Search",
  "description": "Autofocuses the search field of Youtube when landing on the homescreen",
  "icons": {
    "16": "./img/get_started16.png",
    "32": "./img/get_started32.png",
    "48": "./img/get_started48.png",
    "128": "./img/get_started128.png"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["./content.js"],
      "run_at": "document_end"
    }
  ]
}
